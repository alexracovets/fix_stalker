{
  "name": "stalker",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "get:smtp": "ts-node --esm -r dotenv/config src/utils/generateSMTP/generateSMTP.ts"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.70.0",
    "@payloadcms/email-nodemailer": "^3.70.0",
    "@payloadcms/next": "3.70.0",
    "@payloadcms/richtext-lexical": "3.70.0",
    "@payloadcms/ui": "3.70.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@tailwindcss/postcss": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "googleapis": "^170.0.0",
    "graphql": "^16.8.1",
    "immer": "^11.1.3",
    "lucide-react": "^0.562.0",
    "motion": "^12.24.10",
    "next": "15.4.10",
    "payload": "3.70.0",
    "postcss": "^8.5.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-icons": "^5.5.0",
    "sharp": "0.34.2",
    "tailwindcss": "^4.1.18",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@payloadcms/plugin-seo": "^3.70.0",
    "@payloadcms/storage-s3": "^3.70.0",
    "@payloadcms/translations": "^3.70.0",
    "@playwright/test": "latest",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "4.5.2",
    "clsx": "^2.1.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.7",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "prettier": "^3.4.2",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
